@article{schumer_stratigraphic_2011,
	title = {The stratigraphic filter and bias in measurement of geologic rates},
	volume = {38},
	copyright = {Copyright 2011 by the American Geophysical Union.},
	issn = {1944-8007},
	url = {http://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2011GL047118},
	doi = {https://doi.org/10.1029/2011GL047118},
	abstract = {Erosion and deposition rates estimated from the stratigraphic record frequently exhibit a power-law dependence on measurement interval. This dependence can result from a power-law distribution of stratigraphic hiatuses. By representing the stratigraphic filter as a stochastic process called a reverse ascending ladder, we describe a likely origin of power-law hiatuses, and thus, rate scaling. While power-law hiatuses in certain environments can be a direct result of power-law periods of stasis (no deposition or erosion), they are more generally the result of randomness in surface fluctuations irrespective of mean subsidence or uplift. Autocorrelation in fluctuations can make hiatuses more or less heavy-tailed, but still exhibit power-law characteristics. In addition we show that by passing stratigraphic data backward through the filter, certain statistics of surface kinematics from their formative environments can be inferred.},
	language = {en},
	number = {11},
	urldate = {2021-02-21},
	journal = {Geophysical Research Letters},
	author = {Schumer, Rina and Jerolmack, Douglas and McElroy, Brandon},
	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2011GL047118},
	keywords = {erosion, deposition, landscape evolution, stratigraphy, hiatus, power-law},
	file = {Schumer_et_al_2011_Geophysical_Research_Letters.pdf:/Users/ericfoot/Dropbox/research/library/Schumer_et_al/2011/Schumer_et_al_2011_Geophysical_Research_Letters.pdf:application/pdf;Snapshot:/Users/ericfoot/Zotero/storage/ZM7WAEKH/2011GL047118.html:text/html}
}

