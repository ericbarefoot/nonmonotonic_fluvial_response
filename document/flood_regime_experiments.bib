	keywords = {cyclostratigraphy, astronomical forcing, continental paleoenvironments, eccentricity, Madrid Basin, middle Miocene, sedimentary sequence},
	pages = {220--231},
	file = {Abels_et_al_2010_Earth_and_Planetary_Science_Letters.pdf:/Users/ericfoot/Dropbox/research/library/Abels_et_al/2010/Abels_et_al_2010_Earth_and_Planetary_Science_Letters.pdf:application/pdf;ScienceDirect Snapshot:/Users/ericfoot/Zotero/storage/U5DDZEUU/S0012821X0900658X.html:text/html}
