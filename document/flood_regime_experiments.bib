@article{toby_stratigraphic_2019,
	title = {A {Stratigraphic} {Framework} for the {Preservation} and {Shredding} of {Environmental} {Signals}},
	volume = {46},
	copyright = {©2019. The Authors.},
	issn = {1944-8007},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2019GL082555},
	doi = {10.1029/2019GL082555},
	abstract = {The stratigraphic record contains unique information about past landscapes and environmental change. Whether landscapes faithfully transmit signals of environmental change to stratigraphy is unknown because autogenic processes, such as river avulsion, can obscure signals prior to long-term stratigraphic storage. We develop a theoretical framework that predicts when a sediment flux signal will be transferred from the landscape to stratigraphy. This threshold magnitude is a function of signal duration. The magnitude is set by the maximum rate of autogenic volume change of the landscape, which decreases with increasing time window. Physical delta experiments, specifically designed to test our theory, demonstrate that only sediment supply signals with a magnitude greater than the threshold are stored in stratigraphy, supporting our theory. This framework allows us to assess the fidelity of the stratigraphic record to archive past signals of environmental change and predict the short- and long-term impact of current anthropogenic forcing on landscapes.},
	language = {en},
	number = {11},
	urldate = {2020-10-08},
	journal = {Geophysical Research Letters},
	author = {Toby, Stephan C. and Duller, Robert A. and Angelis, Silvio De and Straub, Kyle M.},
	year = {2019},
	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2019GL082555},
	keywords = {morphodynamics, stratigraphy, environmental signals, autogenic and allogenic processes, sediment supply/sediment flux, signal shredding},
	pages = {5837--5845},
	file = {Snapshot:/Users/ericfoot/Zotero/storage/RTJ38XJT/2019GL082555.html:text/html;Toby_et_al_2019_Geophysical_Research_Letters.pdf:/Users/ericfoot/Dropbox/research/library/Toby_et_al/2019/Toby_et_al_2019_Geophysical_Research_Letters.pdf:application/pdf}
