@article{allen_patterns_2015,
	title = {Patterns of river width and surface area revealed by the satellite-derived {North} {American} {River} {Width} data set},
	volume = {42},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/2014GL062764/abstract},
	doi = {10.1002/2014GL062764},
	abstract = {As hydraulic, hydrologic, and biogeochemical models evolve toward greater spatial resolution and larger extent, robust morphometric data sets are essential to constrain their results. Here we present the Landsat-derived North American River Width (NARWidth) data set, the first fine-resolution, continental scale river centerline and width database. NARWidth contains measurements of {\textgreater}2.4 × 105 km of rivers wider than 30 m at mean annual discharge. We find that conventional digital elevation model-derived width data sets underestimate the abundance of wide rivers. To calculate the total surface area of North American rivers, we extrapolate the strong observed relationship between river width and total surface area at different river widths (r2 {\textgreater} 0.99 for 100–2000 m widths) to narrower rivers and streams. We conservatively estimate the total surface area of North American rivers as 1.24−0.15+0.39 × 105 km2 (1σ confidence intervals), values 20−15+38\% greater than previous estimates used to evaluate greenhouse gas efflux from rivers to the atmosphere.},
	language = {en},
	number = {2},
	urldate = {2016-04-27},
	journal = {Geophysical Research Letters},
	author = {Allen, George H. and Pavelsky, Tamlin M.},
	month = jan,
	year = {2015},
	keywords = {1640 Remote sensing, 1825 Geomorphology: fluvial, 1839 Hydrologic scaling, 1856 River channels, fluvial geomorphology, hydrologic scaling, remote sensing, river channels, river surface area, river width},
	pages = {2014GL062764},
	file = {Allen_Pavelsky_2015_Geophysical_Research_Letters.pdf:/Users/ericfoot/Dropbox/research/library/Allen_Pavelsky/2015/Allen_Pavelsky_2015_Geophysical_Research_Letters.pdf:application/pdf;Allen_Pavelsky_2015_Geophysical_Research_Letters.pdf:/Users/ericfoot/Dropbox/research/library/Allen_Pavelsky/Allen_Pavelsky_2015_Geophysical_Research_Letters.pdf:application/pdf;Allen_Pavelsky_2015_Geophysical_Research_Letters.pdf:/Users/ericfoot/Dropbox/research/library/Allen_Pavelsky/Allen_Pavelsky_2015_Geophysical_Research_Letters.pdf:application/pdf;Snapshot:/Users/ericfoot/Zotero/storage/KCFDHKJ9/abstract.html:text/html}
