@article{clark_new_2013,
	title = {New views on “old” carbon in the {Amazon} {River}: {Insight} from the source of organic carbon eroded from the {Peruvian} {Andes}},
	volume = {14},
	issn = {1525-2027},
	shorttitle = {New views on “old” carbon in the {Amazon} {River}},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/ggge.20122/abstract},
	doi = {10.1002/ggge.20122},
	abstract = {Mountain rivers play a key role in the delivery of particulate organic carbon (POC) to large river systems and the ocean. Due to the extent of its drainage area and runoff, the Amazon River is one of Earth's most important biogeochemical systems. However, the source of POC eroded from the humid region of the Eastern Andes and the input of fossil POC from sedimentary rocks (POCfossil) remains poorly constrained. Here we collected suspended sediments from the Kosñipata River during flood events to better characterize Andean POC, measuring the nitrogen to organic carbon ratio (N/C), stable carbon isotopes (δ13Corg) and radiocarbon (Δ14Corg). Δ14Corg values ranged from −711‰ to −15‰, and significant linear trends between Δ14Corg, N/C and δ13Corg suggested that this reflects the mixing of POCfossil with very young organic matter (Δ14Corg {\textasciitilde} 50‰) from the terrestrial biosphere (POCnon-fossil). Using N/C and Δ14Corg in an end-member mixing analysis, we quantify the fraction of POCfossil (to within 0.1) and find that it contributes a constant proportion of the suspended sediment mass (0.37 ± 0.03\%) and up to 80\% of total POC. In contrast, the relative contribution of POCnon-fossil was variable, being most important during the rising limb and peak discharges of flood events. The new data shed light on published measurements of “old” POC (low Δ14Corg) in Andean-fed tributaries of the Amazon River, with their Δ14Corg and δ13Corg values consistent with variable addition of POCfossil. The findings suggest a greater persistence of Andean POC in the lowland Amazon than previously recognized.},
	language = {en},
	number = {5},
	journal = {Geochemistry, Geophysics, Geosystems},
	author = {Clark, K. E. and Hilton, R. G. and West, A. J. and Malhi, Y. and Gröcke, D. R. and Bryant, C. L. and Ascough, P. L. and Robles Caceres, A. and New, M.},
	month = may,
	year = {2013},
	keywords = {1815 Erosion, organic carbon, erosion, Andes, Amazon, radiocarbon, 0428 Carbon cycling, 1055 Organic and biogenic geochemistry, 1040 Radiogenic isotope geochemistry, 9360 South America},
	pages = {1644--1659},
	file = {Clark_et_al_2013_Geochemistry,_Geophysics,_Geosystems.pdf:/Users/ericfoot/Dropbox/research/library/Clark_et_al/2013/Clark_et_al_2013_Geochemistry,_Geophysics,_Geosystems.pdf:application/pdf;Snapshot:/Users/ericfoot/Zotero/storage/4KPTHC2E/abstract.html:text/html}
}

@article{aalto_spatial_2008,
	title = {Spatial and temporal dynamics of sediment accumulation and exchange along {Strickland} {River} floodplains ({Papua} {New} {Guinea}) over decadal-to-centennial timescales},
	volume = {113},
	issn = {2156-2202},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2006JF000627/abstract},
	doi = {10.1029/2006JF000627},
	abstract = {We investigated the processes of sediment exchange between the Strickland River and its lowland floodplain, documenting (1) the rates, textures, and distributions of sediment accumulation, (2) temporal variations in these rates, (3) the remobilization of sediment by erosive processes, and (4) whether net storage is significant over century timescales. We used 210Pb geochronology of floodplain cores from 11 transects to measure deposition rates over the past ∼65 years, finding a decline from 5.5 cm/a (0–10 m distance from the channel) to 0.9 cm/a (∼400 m) to ∼0.1 cm/a ({\textgreater}1 km). Rates are elevated along curved sections of channel and are stable over time, and episodic accumulation is prevalent and may be correlated to floods. Integrated temporally and spatially, the average rate is 1.6 cm/a. Integrating along both sides of ∼318 river km we studied for the lowland Strickland, we calculate 12–19 Mt of annual sediment accumulation (∼0.05 Mt/km or 0.07\% of the annual load per km of river length), representing 17–27\% of the total annual sediment flux. We used georeferenced Landsat images (1972–2001) to quantify channel migration and the resulting return of sediment to the channel. Mean lateral migration was 5.1 ± 0.8 m/a. Given the floodplain width of ∼10 km, this implies a waiting time of ∼1 ka between floodplain formation and subsequent reentrainment of the bank as the channel migrates laterally. This raises the possibility that the net return of material from the floodplain due to channel migration could balance the overbank deposition we observed. The exchange flux between cut bank erosion and point bar deposition is 20–40 Mt, highlighting the significance of sediment recycling (∼50\% of the total load). Such sediment trapping and recycling affects the transport, storage, and evolution of biogeochemically reactive particles, the evolution of the floodplain, and the morphodynamics of basin infilling.},
	language = {en},
	number = {F1},
	journal = {Journal of Geophysical Research: Earth Surface},
	author = {Aalto, Rolf and Lauer, J. Wesley and Dietrich, William E.},
	month = mar,
	year = {2008},
	keywords = {1820 Hydrology: Floodplain dynamics, 1856 Hydrology: River channels, 1825 Hydrology: Geomorphology: fluvial, Strickland River, PNG, floodplain accumulation, channel migration, 1165 Geochronology: Sedimentary geochronology, 1115 Geochronology: Radioisotope geochronology},
	pages = {F01S04},
	file = {Aalto_et_al_2008_Journal_of_Geophysical_Research_Earth_Surface.pdf:/Users/ericfoot/Dropbox/research/library/Aalto_et_al/2008/Aalto_et_al_2008_Journal_of_Geophysical_Research_Earth_Surface.pdf:application/pdf;Snapshot:/Users/ericfoot/Zotero/storage/D869GZEK/abstract.html:text/html}
