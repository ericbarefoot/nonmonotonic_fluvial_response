	file = {Bufe_et_al_2016_Nature_Geoscience.pdf:/Users/ericfoot/Dropbox/research/library/Bufe_et_al/2016/Bufe_et_al_2016_Nature_Geoscience.pdf:application/pdf;Bufe_et_al_2016_Nature_Geoscience.pdf:/Users/ericfoot/Dropbox/research/library/Bufe_et_al/2016/Bufe_et_al_2016_Nature_Geoscience2.pdf:application/pdf;Snapshot:/Users/ericfoot/Zotero/storage/583XNH3X/ngeo2773.html:text/html;Snapshot:/Users/ericfoot/Zotero/storage/4TAVYC5V/ngeo2773.html:text/html}
